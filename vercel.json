{
  "version": 2,
  "public": true,
  "buildCommand": "cd front && npm install && npm run build -- --configuration production",
  "outputDirectory": "front/dist/catan",
  "cleanUrls": true,
  "rewrites": [
    { "source": "/assets/(.*)", "destination": "/browser/assets/$1" },
    { "source": "/(.*)", "destination": "/browser/index.html" }
  ]
} 