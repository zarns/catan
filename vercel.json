{
  "version": 2,
  "public": true,
  "buildCommand": "cd front && npm install && npm run build -- --configuration production",
  "outputDirectory": "front/dist/catan/browser",
  "routes": [
    { "handle": "filesystem" },
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.*\\.[a-z0-9]+$)", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
} 