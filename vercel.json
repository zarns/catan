{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "public": true,
  "buildCommand": "npm install && npm run build -- --configuration production",
  "outputDirectory": "dist/catan",
  "framework": "angular",
  "rewrites": [
    { "source": "/(.*)", "destination": "/browser/$1" }
  ]
} 